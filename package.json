{
  "name": "pwa-dicom-viewer",
  "version": "1.0.0",
  "description": "DICOM format viewer as PWA",
  "main": "index.js",
  "scripts": {
    "ttest": "mocha --compilers ts:ts-node/register,tsx:ts-node/register --reporter list \"test/**/*.test.ts\" ",
    "test": "mocha-webpack --webpack-config webpack-test.config.js \"**/*.test.ts\"",
    "test:watch": "mocha-webpack --webpack-config webpack-test.config.js --watch --reporter list \"**/*.test.ts\"",
    "lint:client": "tslint -c tslint.json src/client/**/*.{ts,tsx}",
    "lint:server": "tslint -c tslint.json src/server/**/*.{ts,tsx}",
    "lint": "npm run lint:server && npm run lint:client",
    "start": "webpack-dev-server --config config/webpack.config.js --inline",
    "build": "webpack --config config/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://milung.visualstudio.com/DefaultCollection/_git/SDA-PwaDicomViewer"
  },
  "keywords": [
    "DICOM",
    "PWA"
  ],
  "author": "SDA team",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/classnames": "^2.2.0",
    "@types/enzyme": "^2.8.2",
    "@types/express": "^4.0.36",
    "@types/lodash": "^4.14.69",
    "@types/material-ui": "^0.17.16",
    "@types/mocha": "^2.2.41",
    "@types/react": "^15.0.38",
    "@types/react-dom": "^15.5.1",
    "@types/react-tap-event-plugin": "0.0.30",
    "chai": "^4.1.0",
    "classnames": "^2.2.5",
    "enzyme": "^2.9.1",
    "html-webpack-plugin": "^2.29.0",
    "material-ui": "^0.18.6",
    "mocha": "^3.4.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-tap-event-plugin": "^2.0.1",
    "source-map-loader": "^0.2.1",
    "ts-loader": "^2.3.0",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.4.1",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "express": "^4.15.3"
  }
}
